{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Run",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"start",
				"debug"
			],
			"port": 9229,
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"name": "Server",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "Build Server",
			"program": "${workspaceFolder}/AngularLibrary/Server/bin/Debug/AngularLibrary.dll",
			"args": [],
			"cwd": "${workspaceFolder}/AngularLibrary",
			"stopAtEntry": false,
			"internalConsoleOptions": "openOnSessionStart",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		}
	]
}